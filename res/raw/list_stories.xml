<?xml version="1.0" encoding="UTF-8"?>
<resources>
    <Lesson
        name="Animals">
        <Scene>
            <Background
                src="animals_tutt"/>
            <Character
                name="main"
                expression="default"
                animate="entranceFromLeft"
                say="This is quite the adventure right?"
                soundfile="narration_animals_1"/>
        </Scene>
        <Scene>
            <Character
                name="main"
                say="Let's just rest for a while."
                soundfile="narration_animals_2"/>
        </Scene>
        <Scene>
            <Character
                name="main"
                expression="point"
                say="(Alam ko na!/I got it!)"
                soundfile="narration_animals_3"/>
        </Scene>
        <Scene>
            <Character
                name="main"
                expression="question"
                say="Hi! Can we go inside?"
                soundfile="narration_animals_4"/>
        </Scene>
        <Scene>
            <Character
                name="guard"
                animate="entranceFromRight"
                say="Sure! It's 2000 pesos each."
                soundfile="narration_animals_5"/>
        </Scene>
        <Scene>
            <Character
                name="main"
                expression="shocked"
                say="What? We don't have that much."
                soundfile="narration_animals_6"/>
        </Scene>
        <Scene>
            <Character
                name="main"
                say="(username), what do we do now?"
                soundfile="narration_animals_7"/>
        </Scene>
        <Scene>
            <Character
                name="guard"
                expression="waisttalk"
                say="If you can answer my quiz, I will let you in for free."
                soundfile="narration_animals_8"/>
        </Scene>
        <Scene>
            <Character
                name="guard"
                expression="wave"
                say="Don't worry, (madali lang/it's easy)."
                soundfile="narration_animals_9"/>
        </Scene>
        <Scene>
            <Character
                name="main"
                expression="point"
                say="(Talaga?/Really?)"
                soundfile="narration_animals_10"/>
        </Scene>
        <Scene>
            <Character
                name="main"
                expression="question"
                say="But, can we review first?"
                soundfile="narration_animals_11"/>
        </Scene>
        <Scene>
            <Character
                name="guard"
                expression="wavetalk"
                say="Ok!"
                soundfile="narration_animals_12"/>
        </Scene>
    </Lesson>
    
    <Lesson
        name="Cooking">
        
        <Scene>
            <Background src="colors_tutt"/>
            <Character
                name="main"
                animate="entranceFromLeft"
                expression="question"
                say="Hey where are we?"
                soundfile="narration_cooking_1"/>
        </Scene>
        <Scene>
            <Character
                name="main"
                expression="shocked"
                />
            <Character
                name="chef"
                animate="entranceFromLeft"
                expression="armscross"
                say="You're in my kitchen!"
                soundfile="narration_cooking_2"/>
        </Scene>
        <Scene>
            <Character
                name="chef"
                say="What are you doing here?!"
                soundfile="narration_cooking_3"/>
        </Scene>
        <Scene>
            <Character
                name="chef"
                expression="talk"
                say="(Di bale/No matter), I have better things to do."
                soundfile="narration_cooking_4"/>
        </Scene>
        <Scene>
            <Character
                name="chef"
                say="(Halika!/Come!) Help me!"
                soundfile="narration_cooking_5"/>
        </Scene>
        <Scene>
            <Character
                name="main"
                expression="shocked"
                say="I don't think we have a choice here (username)"
                soundfile="narration_cooking_6"/>
        </Scene>
        <Scene>
            <Character
                name="main"
                say="(Tara!/Let's go!)"
                soundfile="narration_cooking_7"/>
        </Scene>
        <Scene>
            <Character
                name="chef"
                expression="armscross"
                say="Are you ready? Ok! I'm only gonna do this once."
                soundfile="narration_cooking_8"
                animate="exitToRight"/>
        </Scene>
	</Lesson>
	
    <Lesson
        name="Family">
        <Scene>
            <Background src="family_tutt"/>
	        <Character
	            name="main"
	            animate="entranceFromLeft"
	            say="Hi (username)!"
                soundfile="narration_family_1"/>
        </Scene>
        <Scene>
	        <Character
	            name="main"
	            animate="jump"
	            say="I am (maincharacter)."
                soundfile="narration_family_2"/>
	    </Scene>
        <Scene>
	        <Character
	            name="main"
	            expression="point"
	            say="Welcome to my house!"
                soundfile="narration_family_3"/>
        </Scene>
        <Scene>
            <Character
                name="main"
                expression="default"
                say="Ready to start our adventure?"
                soundfile="narration_family_4"/>
        </Scene>
        <Scene>
            <Character
                name="main"
                expression="shocked"/>
            <Character
                name="nanay"
                animate="entranceFromRight"
                say="(maincharacter)! (Kain na!/Let's eat!) You can't go out with an empty stomach!"
                soundfile="narration_family_5"/>
        </Scene>
        <Scene>
            <Character
                name="main"
                expression="default"
                say="Oh ok nanay"
                soundfile="narration_family_6"/>
            <Character
                name="nanay"
                animate="exitToRight"/>
        </Scene>
        <Scene>
            <Character
                name="main"
                expression="question"
                say="Well, just like nanay said, let's eat!"
                soundfile="narration_family_7"/>
        </Scene>
        <Scene>
            <Character
                name="main"
                expression="point"
                say="Come on. I'll introduce you to the others."
                soundfile="narration_family_8"/>
        </Scene>
    </Lesson>
    
    <Lesson
        name="House">
        <Scene>
            <Background src="house_tutt"/>
            <Character 
                name="main"
                animate="entranceFromLeft"
                say="Hey (username). (Halika!/Come!) I want you to meet my cousin."
                soundfile="narration_house_1"/>
        </Scene>
        <Scene>
            <Character
                name="main"
                expression="point"
                say="Her name is Pichi-Pichi."
                soundfile="narration_house_2"/>
        </Scene>
        <Scene>
            <Character
                name="main"
                expression="default"/>
            <Character
                name="pichi"
                animate="entranceFromLeft"
                say="Hi (username)! (Pasok tayo/Let's go inside), I'll show you something."
                soundfile="narration_house_3"/>
        </Scene>
        <Scene>
            <Character
                name="pichi"
                expression="hold"
                say="You saw my house earlier right?"
                soundfile="narration_house_4"/>
        </Scene>
        <Scene>
            <Character
                name="pichi"
                expression="holdtalk"
                say="Well I tried to make it by gluing things together..."
                soundfile="narration_house_5"/>
        </Scene>
        <Scene>
            <Character
                name="pichi"
                say="... and look!"
                soundfile="narration_house_6"/>
            <Character
                name="house"
                animate="entranceFromAbove"/>
        </Scene>
        <Scene>
            <Character
                name="pichi"
                expression="shocked"/>
            <Character
                name="house"
                animate="exitToBelow"/>
        </Scene>
        <Scene>
            <Character
                name="main"
                expression="shocked"
                say="(Hala!/Oh no!)  The pieces fell off!"
                soundfile="narration_house_7"/>
        </Scene>
        <Scene>
            <Character
                name="pichi"
                animate="exitToRight"
                say="(username) (Patulong/Help me!)"
                soundfile="narration_house_8"/>
        </Scene>
        <Scene>
            <Character
                name="main"
                expression="default"
                say="Let’s help Pichi."
                soundfile="narration_house_9"/>
        </Scene>
        <Scene>
            <Character
                name="main"
                say="Here I’ll give you the pieces."
                soundfile="narration_house_10"/>
        </Scene>
    </Lesson>
    
    <Lesson
        name="Shape">
    	<Scene>
    	    <Background src="shapes_tutt"/>
    	    <Character
    	        name="main"
    	        animate="entranceFromLeft"
    	        expression="point"
    	        say="Wow we finally got away from that chef."
                soundfile="narration_shape_1"/>
    	</Scene>
    	<Scene>
    	    <Character
    	        name="main"
    	        say="Now we're in a rocket ship! Excited (na ako!/I'm already excited!)"
                soundfile="narration_shape_2"/>
    	</Scene>
    	<Scene>
    	    <Character
    	        name="main"
    	        expression="question"
    	        say="What's going on here?"
                soundfile="narration_shape_3"/>
    	</Scene>
    	<Scene>
    	    <Character
    	        name="crew"
				animate="entranceFromRight"
				say="One of our actors for the show is sick! Can you help us with the show?"
                soundfile="narration_shape_4"/>
    	</Scene>
    	<Scene>
    	    <Character
    	        name="main"
    	        expression="default"
    	        say="I guess a little acting won't hurt."
                soundfile="narration_shape_5"/>
    	    <Character
    	        name="crew"
    	        animate="exitToRight"/>
    	</Scene>
    	<Scene>
    	    <Character
    	        name="main"
    	        expression="shocked"
    	        say="(Tignan mo!/ Look!)  Someone is on the screen (username)"
                soundfile="narration_shape_6"/>
    	</Scene>
    	<Scene>
    	    <Background src="shapes_alien"/>
    	    <Character 
    	        name="crew"
    	        say="WORLD DOMINATION"
                soundfile="narration_shape_7"/>
    	</Scene>
    	<Scene>
    	    <Character
    	        name="main"
    	        animate="panic"
    	        say="(Naku!/Oh no!) We have to defeat the aliens quickly!"
                soundfile="narration_shape_8"/>
    	</Scene>
    	<Scene>
    	    <Character
    	        name="main"
    	        say="Let's figure out what these buttons do."
                soundfile="narration_shape_9"/>
    	</Scene>
    </Lesson>
