	     </RelativeLayout>
	     
	    </HorizontalScrollView>
	  					 	<ImageView
	  						android:id="@+id/swipe_ind"
                			android:layout_width="wrap_content"
                			android:layout_height="wrap_content"
                			android:src="@drawable/swipe_left"
            				android:layout_alignParentRight="true"
            				android:scaleX="0.5"
            				android:scaleY="0.5"
                			/>
	  					
	  					<ImageView
	  					    android:id="@+id/img_itemLabel"
	  					    android:layout_width="wrap_content"
	  					    android:layout_height="wrap_content"
	  					    android:gravity="center_horizontal"
	  					    android:layout_marginTop="30dp"
	  					    android:background="@null"
	  					    />
	  					
	  					<TextView
	  					    android:id="@+id/tv_itemlabel"
	  					    android:layout_width="match_parent"
	  					    android:layout_height="match_parent"
	  					    android:textAlignment="center"
	  					    android:textColor="#252525"
	  					    android:textSize="25sp"
	  					    android:layout_alignRight="@id/img_itemLabel"
	  					    android:layout_alignLeft="@id/img_itemLabel"
	  					    android:layout_alignTop="@id/img_itemLabel"
	  					    android:layout_alignBottom="@+d/img_itemLabel"
	  					    android:paddingTop="20dp"
	  					    android:paddingRight="30dp"
	  					    android:paddingBottom="20dp"
	       					android:paddingLeft="180dp"
	  					 />
	  					
			
         				<TextView
	             			android:id="@+id/tv_feedback"
                			android:layout_width="match_parent"
                			android:layout_height="120dp"
                			android:layout_marginLeft="50dp"
                			android:layout_marginRight="50dp"
                			android:text="text"
                			android:layout_alignParentBottom="true"
            				android:layout_centerHorizontal="true"
            				android:gravity="center"
            				android:layout_marginBottom="20dp"
            				android:layout_marginTop="40dp"
            			 	android:background="@drawable/feedbackbox"
                			android:textSize="29sp"
	        				android:textColor="#2c2c2c" />
                				
